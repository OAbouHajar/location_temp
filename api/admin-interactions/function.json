{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "options"],
      "route": "admin/interactions"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "interactions",
      "databaseName": "LocationTracker",
      "collectionName": "Interactions",
      "connectionStringSetting": "CosmosDbConnectionString",
      "sqlQuery": "SELECT * FROM c ORDER BY c.serverTimestamp DESC"
    }
  ]
}
