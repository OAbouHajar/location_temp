{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post", "options"],
      "route": "interaction"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "LocationTracker",
      "collectionName": "Interactions",
      "createIfNotExists": true,
      "connectionStringSetting": "CosmosDbConnectionString"
    }
  ]
}
