{
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin.html"
    },
    {
      "route": "/location-dashboard",
      "rewrite": "/location-dashboard.html"
    },
    {
      "route": "/free-dashboard",
      "rewrite": "/free-dashboard.html"
    },
    {
      "route": "/dashboard",
      "rewrite": "/free-dashboard.html"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/admin.html", "/admin", "/location-dashboard.html", "/location-dashboard", "/free-dashboard.html", "/free-dashboard", "/dashboard"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html"
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization"
  }
}
